{
  "name": "nestjs-articles-project",
  "version": "1.0.0",
  "description": "Teste Sistema Simples de Artigos - NestJS",
  "main": "dist/main.js",
  "scripts": {
    "prestart:prod": "npm run build",
    "start:dev": "nest start --watch",
    "build": "tsc -p tsconfig.build.json",
    "start:prod": "node dist/main.js",
    "seed": "node dist/seed.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "typeorm migration:generate -d dist/data-source.js src/migrations/CreateInitialTables",
    "migration:run": "node ./node_modules/typeorm/cli.js migration:run -d dist/data-source.js",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.3.12",
    "pg": "^8.9.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0",
    "sqlite3": "^5.1.6"
  }
}